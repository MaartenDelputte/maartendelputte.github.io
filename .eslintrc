{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": false,
    "es2017": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "strict": [
      "error",
      "global"
    ],
    "curly": "warn"
  }
}
